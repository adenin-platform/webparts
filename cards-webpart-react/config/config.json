{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "adenin-digital-assistant-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/adeninDigitalAssistant/AdeninDigitalAssistantWebPart.js",
          "manifest": "./src/webparts/adeninDigitalAssistant/AdeninDigitalAssistantWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "adaptive-expressions": "node_modules/adaptive-expressions/",
    "adaptivecards-templating": "node_modules/adaptivecards-templating/dist/adaptivecards-templating.min.js",
    "adaptivecards": "node_modules/adaptivecards/dist/adaptivecards.min.js",
    "markdown-it": "node_modules/markdown-it/dist/markdown-it.min.js"
  },
  "localizedResources": {
    "AdeninDigitalAssistantWebPartStrings": "lib/webparts/adeninDigitalAssistant/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}